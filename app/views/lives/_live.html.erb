<div id="<%= dom_id live %>">
  <p>
    <strong>Name:</strong>
    <%= live.name %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= live.date %>
  </p>

  <p>
    <strong>Comedians:</strong>
    <ul>
      <% live.comedians.each do |comedian| %>
        <%= render partial: "lives/comedian", object: comedian %>
      <% end %>
    </ul>
  </p>
</div>
